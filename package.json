    "postcss": "^8.5.6",
    "prettier": "^3.6.2",
    "@primeuix/themes": "^4.0.0",
    "primeicons": "^7.0.0",
    "primevue": "^4.4.1",
    "sass": "^1.94.2",
    "typescript": "^5.9.3",
    "vite": "^7.2.4",
    "vite-plugin-vue-devtools": "^7.7.7",
    "vite-tsconfig-paths": "^5.1.4",
    "vue": "^3.5.25",
