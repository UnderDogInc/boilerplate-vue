{
  "name": "vue-project-template",
  "version": "0.0.1",
  "author": "",
  "type": "module",
  "private": true,
  "browserslist": [
    "cover 99.5%"
  ],
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview --port 4173",
    "lint": "eslint src --fix",
