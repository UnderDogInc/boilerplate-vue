    "husky": "^9.1.7",
    "lint-staged": "^15.2.11",
    "vue-eslint-parser": "^10.2.0",
    "vite-plugin-eslint2": "^5.0.4",
    "@vue/tsconfig": "^0.7.0",
    "@types/node": "^24.10.1",
    "globals": "^16.5.0",
    "jsdom": "^26.1.0",
    "postcss-html": "^1.8.0",
    "stylelint": "^16.26.0",
    "stylelint-config-recess-order": "^7.4.0",
    "stylelint-config-recommended-vue": "^1.6.1",
